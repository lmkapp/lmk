{
  "name": "@lmkapp/jupyter-widget",
  "version": "1.0.3",
  "description": "Jupyter Widget for using LMK in a jupyter notebook",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.{d.ts,js}",
    "dist/*.js",
    "styles/*.css"
  ],
  "homepage": "https://app.lmkapp.dev",
  "bugs": {
    "url": "https://github.com/lmkapp/lmk/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Cam Feenstra",
    "email": "me@camfeenstra.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/lmkapp/lmk/blob/main/packages/python-client/npm.README.md"
  },
  "scripts": {
    "build": "pnpm build:lib && pnpm build:nbextension && pnpm build:labextension:dev",
    "build:prod": "pnpm run build:lib && NODE_ENV=production pnpm build:nbextension && NODE_ENV=production pnpm build:labextension",
    "build:python": "pnpm clean && pnpm generate && venv/bin/python -m build .",
    "build:publish": "pnpm clean && pnpm build:prod",
    "build:labextension": "jupyter labextension build . && cd lmk/jupyter/labextension",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:nbextension": "webpack",
    "build:docs": "novella -b $(pwd)/../docs",
    "dev:docs": "novella -b $(pwd)/../docs --use-reloader",
    "clean": "pnpm clean:dist && pnpm run clean:lib && pnpm run clean:nbextension && pnpm run clean:labextension && pnpm run clean:generated && pnpm run clean:python",
    "clean:dist": "rimraf dist",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf lmk/jupyter/labextension",
    "clean:nbextension": "rimraf lmk/jupyter/nbextension",
    "clean:generated": "rimraf lmk/generated",
    "clean:python": "rm -rf lmk/**/__pycache__ lmk/**/*.pyc",
    "clean:docs": "rm -rf ../docs/docs/python/reference.md",
    "format": "prettier --write \"src/**/*.ts\" && venv/bin/black lmk",
    "generate": "openapi-generator-cli generate > /dev/null",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "prepack": "pnpm run build:lib",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w -p tsconfig.build.json",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch .",
    "install-all": "pnpm install && venv/bin/pip install -e '.[dev,publish,jupyter]'",
    "readme:npm": "mv README.md git.README.md && mv npm.README.md README.md",
    "readme:git": "mv README.md npm.README.md && mv git.README.md README.md",
    "pypi-check": "twine check dist/lmkapp-*",
    "pypi-upload-test": "twine upload --repository-url https://test.pypi.org/legacy/ dist/lmkapp-*",
    "pypi-upload": "twine upload dist/lmkapp-*",
    "test:python": "venv/bin/py.test -vv tests",
    "check:python": "venv/bin/mypy && venv/bin/black --check lmk",
    "tox": "venv/bin/tox",
    "npm-publish": "./publish.sh",
    "test:blah": "echo HERE $([[ $(cat package.json | jq -r .version) = \"0\" ]] && echo abc || echo def)"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10 || ^2 || ^3 || ^4 || ^5 || ^6",
    "classnames": "^2.3.2",
    "uuid": "^9.0.0",
    "yjs": "^13.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@jupyter-widgets/base-manager": "^1.0.2",
    "@jupyterlab/builder": "^3.0.0",
    "@jupyterlab/services": "^6.6.1",
    "@jupyterlab/ui-components": "^3.6.1",
    "@lumino/application": "^1.6.0",
    "@lumino/widgets": "^1.6.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@swc/core": "^1.3.56",
    "@types/backbone": "^1.4.15",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.18.23",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/uuid": "^9.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "acorn": "^7.2.0",
    "backbone": "^1.4.1",
    "crypto": "1.0.1",
    "css-loader": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "esm": "^3.2.25",
    "fs-extra": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "mkdirp": "^0.5.1",
    "ms": "^2.1.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.0",
    "prettier": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^2.6.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.0.0",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^8.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.1.3",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.0.0"
  },
  "jupyterlab": {
    "extension": "dist/plugin",
    "outputDir": "lmk/jupyter/labextension/@lmkapp/jupyter-widget",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyterlab/ui-components": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
